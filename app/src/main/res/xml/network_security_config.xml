<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config>
        <domain includeSubdomains="true">nope.sh</domain>
        <trust-anchors>
            <certificates src="@raw/ca"/>
        </trust-anchors>
    </domain-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">http://localhost:9987</domain>
    </domain-config>
    <base-config cleartextTrafficPermitted="true" />
</network-security-config>
